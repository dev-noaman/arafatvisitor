# Set to true to show debug quick-login buttons (Admin, GM, Host, Reception) on the login form.
# Use false in production.
VITE_SHOW_DEBUG_LOGIN=false

# API base URL for backend. Leave empty to use sessionStorage config.
VITE_API_BASE=https://arafatvisitor.cloud/api

# Admin panel URL (AdminJS runs on port 3000). Leave empty to derive from API base.
VITE_ADMIN_URL=https://arafatvisitor.cloud:3000/admin
