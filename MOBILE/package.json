{
  "name": "mobile",
  "version": "1.0.0",
  "description": "Arafat VMS Mobile App - React Native with Expo",
  "author": "Arafat Group",
  "license": "UNLICENSED",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:web": "expo export --platform web",
    "build:android": "expo prebuild --platform android --clean && cd android && ./gradlew assembleRelease",
    "build:ios": "expo prebuild --platform ios --clean && cd ios && pod install && xcodebuild -workspace ArafatVisitor.xcworkspace -scheme ArafatVisitor -configuration Release -sdk iphoneos CODE_SIGN_IDENTITY='' CODE_SIGNING_REQUIRED=NO",
    "test": "echo \"No test runner configured\"",
    "generate-icons": "node scripts/generate-icons.js",
    "lint": "npx tsc --noEmit"
  },
  "dependencies": {
    "@expo-google-fonts/outfit": "^0.4.3",
    "@expo/metro-runtime": "^6.1.2",
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/netinfo": "^11.5.2",
    "@react-navigation/bottom-tabs": "^7.13.0",
    "@react-navigation/native": "^7.1.28",
    "@react-navigation/native-stack": "^7.12.0",
    "@tanstack/react-query": "^5.90.21",
    "axios": "^1.13.5",
    "expo": "~54.0.33",
    "expo-camera": "^17.0.10",
    "expo-constants": "^18.0.13",
    "expo-font": "~14.0.11",
    "expo-haptics": "^15.0.8",
    "expo-notifications": "^0.32.16",
    "expo-secure-store": "^15.0.8",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-dom": "^19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "^2.30.0",
    "react-native-reanimated": "^4.2.1",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "^4.23.0",
    "react-native-web": "^0.21.2",
    "react-native-worklets": "^0.7.3",
    "tailwindcss": "^3.4.19",
    "zod": "^4.3.6",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "babel-preset-expo": "^54.0.10",
    "sharp": "^0.34.5",
    "typescript": "~5.9.2"
  },
  "private": true
}
